<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.media.MediaView?>

<AnchorPane id="AnchorPane" prefHeight="491.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sharks.HammerheadController">
   <children>
      <MediaView fx:id="videoHammerhead" fitHeight="200.0" fitWidth="200.0" layoutX="33.0" layoutY="217.0" />
      <ImageView fx:id="imageHammerhead" fitHeight="159.0" fitWidth="247.0" layoutX="14.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../HammerheadMedia/hammerhead-shark.jpg" />
         </image></ImageView>
      <TextArea fx:id="textHammerhead" layoutX="378.0" layoutY="20.0" prefHeight="326.0" prefWidth="200.0" />
      <ButtonBar layoutX="14.0" layoutY="437.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button fx:id="playButton" mnemonicParsing="false" onAction="#play" text="Play" />
            <Button fx:id="pauseButton" mnemonicParsing="false" onAction="#pause" text="Pause" />
        </buttons>
      </ButtonBar>
      <Button fx:id="backButton" layoutX="470.0" layoutY="445.0" mnemonicParsing="false" onAction="#goToScene1" text="Back to Main" />
   </children>
</AnchorPane>
